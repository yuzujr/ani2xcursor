#: src/cli.cpp:24
msgid ""
"                                24,32  - Ensure sizes (reuse if present, "
"rescale if missing)\n"
msgstr ""
"                                24,32  - 确保尺寸（如果存在则复用，如果缺失则缩放）\n"

#: src/cli.cpp:22
msgid "                                all    - Export all sizes (default)\n"
msgstr "                                all    - 导出所有尺寸（默认）\n"

#: src/cli.cpp:23
msgid "                                max    - Export only largest size\n"
msgstr "                                max    - 仅导出最大尺寸\n"

#: src/cli.cpp:20
msgid ""
"  --format, -f <mode>       Output format: xcursor (default) or source\n"
msgstr "  --format, -f <模式>       输出格式：xcursor（默认）或 source（源码）\n"

#: src/cli.cpp:32
msgid "  --help, -h                Show this help message\n"
msgstr "  --help, -h                显示此帮助信息\n"

#: src/cli.cpp:29
msgid "  --install, -i             Install theme to $XDG_DATA_HOME/icons\n"
msgstr "  --install, -i             安装主题到 $XDG_DATA_HOME/icons\n"

#: src/cli.cpp:27
msgid ""
"  --list, -l                Show available sizes in cursor files then exit\n"
msgstr "  --list, -l                显示光标文件中的可用尺寸并退出\n"

#: src/cli.cpp:26
msgid ""
"  --manifest, -m            Generate previews + manifest.toml then exit\n"
msgstr "  --manifest, -m            生成预览 + manifest.toml 并退出\n"

#: src/cli.cpp:19
msgid "  --out, -o <dir>           Output directory (default: ./out)\n"
msgstr "  --out, -o <目录>          输出目录（默认：./out）\n"

#: src/cli.cpp:21
msgid "  --size, -s <mode>         Size selection mode:\n"
msgstr "  --size, -s <模式>         尺寸选择模式：\n"

#: src/cli.cpp:31
msgid "  --skip-broken             Continue on conversion errors\n"
msgstr "  --skip-broken             遇到转换错误时继续\n"

#: src/cli.cpp:30
msgid "  --verbose, -v             Enable verbose logging\n"
msgstr "  --verbose, -v             启用详细日志\n"

